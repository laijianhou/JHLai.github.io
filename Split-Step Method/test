from math import pi, sqrt
import numpy as np
import matplotlib.pyplot as plt
from matplotlib.animation import FuncAnimation

class Parameter:
  def __init__(self, xmax: float, resol: int, dt: float, timesteps: int, im_time: bool):
    self.xmax = xmax
    self.resl = resol
    self.dt = dt
    self.timesteps = timesteps
    self.im_time = im_time
